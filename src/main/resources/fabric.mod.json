{
  "schemaVersion": 1,
  "id": "loli-utils",
  "version": "${version}",
  "name": "LoliUtils",
  "description": "An addon template for the Meteor addons.",
  "authors": [
    "loli-tummy"
  ],
  "contact": {
    "repo": "https://github.com/loli-tummy/loli-utils"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.bnuyhq.loli_utils.LoliUtils"
    ]
  },
  "mixins": [
    "loli-utils.mixins.json",
    "xaero_loli-utils.mixins.json",
    "meteorclient_loli-utils.mixins.json"
  ],
  "accessWidener": "loli-utils.accesswidener",
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
